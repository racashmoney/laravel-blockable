{
	"name": "racashmoney/laravel-blockable",
	"description": "Trait for Laravel Eloquent models to allow easy implementation of a 'block' or 'ignore' feature.",
	"license": "MIT",
	"homepage": "https://github.com/racashmoney/laravel-blockable",
	"keywords": ["trait", "laravel", "laravel5", "eloquent", "blockable", "ignore", "block"],
	"authors": [
		{
			"name": "Robert Conner",
			"email": "rtconner@smarter.bz"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"illuminate/database": ">=5.0",
		"illuminate/support": ">=5.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3.0",
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "~0.9"
	},
	"autoload": {
		"psr-4": {
			"Racashmoney\\Blockable\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	}
}
